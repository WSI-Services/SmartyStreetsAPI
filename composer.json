{
    "name": "wsiservices/smartystreetsapi",
    "description": "SmartyStreets API Service",
    "type": "library",
    "keywords": [
        "smartystreets",
        "api",
        "services"
    ],
    "license": "GPL-2.0",

    "authors": [
        {
            "name": "Sam Likins",
            "email": "sam.likins@wsi-services.com",
            "homepage": "http://wsi-services.com/"
        }
    ],

    "require": {
        "php": ">=5.3.0",
        "psr/log": "1.0.*"
    },
    "autoload": {
        "psr-4": {
            "WSIServices\\SmartyStreetsAPI\\": "source/"
        }
    },

    "require-dev": {
        "phploc/phploc": "*",
        "phpunit/phpunit": "~4.1",
        "phpdocumentor/phpdocumentor": "2.*",
        "mockery/mockery": "0.9.*",
        "illuminate/contracts": "5.1.*",
        "illuminate/support": "5.1.*",
        "laravel/homestead": "^2.1",
        "ext-http": "*",
        "guzzlehttp/guzzle": "5.*"
    },
    "autoload-dev": {
        "psr-4": {
            "WSIServices\\SmartyStreetsAPI\\Tests\\": "tests/source"
        }
    },

    "suggest": {
        "ext-http": "Provides support for API request driver [PeclHttp]",
        "guzzlehttp/guzzle": "Provides support for API request driver [GuzzleHttp]"
    },
    "minimum-stability": "dev"
}
